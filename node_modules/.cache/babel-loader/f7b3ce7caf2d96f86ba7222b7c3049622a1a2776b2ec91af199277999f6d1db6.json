   },\r\n\r\n}\r\n\r\n</script>\r\n\r\n\r\n<style lang=\"scss\">\r\n@import url('https://fonts.googleapis.com/css2?family=Cairo&family=Marhey:wght@300;400;500;600;700&family=Montserrat:wght@100;200;300;400;500;600;700;800;900&family=Open+Sans:wght@300;400;700&family=Poppins:wght@100&family=Roboto:ital,wght@0,100;0,300;0,400;0,500;1,300&family=Work+Sans:wght@300;400;500;600;700;800&display=swap');\r\n\r\n:root {\r\n    --content-width: 1000px;\r\n    --breakpoint: 799px;\r\n    --nav-hieght: 70px;\r\n    --background-color: #00a19b;\r\n    --fontM-color: #00a19b;\r\n    --font-color: #ffffff;\r\n    --link-hover-color: #2581dc\r\n}\r\n\r\n* {\r\n    font-family: 'Marhey', sans-serif;\r\n    font-size: 26px;\r\n    font-weight: 400;\r\n}\r\n\r\n\r\n.navigation {\r\n    width: 100%;\r\n    transition: top 0.3s;\r\n    padding: 30px;\r\n    background-color: #ffffff;\r\n    box-shadow: 0 0 8px #f6e8e8;\r\n\r\n    &.onScroll {\r\n        box-shadow: 0 0 20px #aaa;\r\n        background-color: white;\r\n\r\n        ul li a {\r\n            color: black;\r\n        }\r\n\r\n        ul li  .active {\r\n            color: #00a19b;\r\n        }\r\n\r\n        .bar1,\r\n        .bar2,\r\n        .bar3 {\r\n            background-color: black;\r\n        }\r\n        .respon ul {\r\n            top: 110px;\r\n        }\r\n    }\r\n}\r\n\r\n\r\n.logo {\r\n    position: relative;\r\n    padding-left: 20px;\r\n    float: left;\r\n}\r\n\r\n.logo img {\r\n    position: absolute;\r\n    width: 100px;\r\n    height: 100px;\r\n    display: block;\r\n    padding-top: -0px;\r\n\r\n\r\n}\r\n\r\nnav {\r\n    float: right;\r\n    padding: 0 !important;\r\n\r\n    \r\n\r\n    ul {\r\n        list-style: none;\r\n        margin: 0;\r\n        padding: 0;\r\n\r\n        \r\n\r\n        a,\r\n            a:visited {\r\n                display: block;\r\n                padding: 0 26px;\r\n                line-height: var(--nav-hieght);\r\n                color: #00a19b;\r\n                text-decoration: none;\r\n\r\n                \r\n            }\r\n\r\n            a:hover {\r\n                    margin-top: -15px;\r\n                    transition: all 0.3s;\r\n                }\r\n\r\n            \r\n        li {\r\n            float: left;\r\n            position: relative;\r\n            margin-left: -20px;\r\n            padding-top: 5px;\r\n\r\n            span {\r\n                padding-top: 5px;\r\n            }\r\n\r\n            .card-icon {\r\n                padding-bottom: 0px;\r\n            }\r\n\r\n            .house-icon {\r\n                padding-bottom: 2px;\r\n            }\r\n\r\n            .active {\r\n                color: black;\r\n            }\r\n        }\r\n    }\r\n\r\n\r\n}\r\n\r\n.icon {\r\n    text-align: -webkit-right;\r\n}\r\n\r\n.icon:hover {\r\n    .bar2 {\r\n        width: 35px;\r\n    }\r\n}\r\n\r\n.bar1,\r\n.bar3 {\r\n    width: 35px;\r\n    height: 5px;\r\n    background-color: #00a19b;\r\n    margin: 6px 0;\r\n    transition: 0.4s;\r\n    border-radius: 4px;\r\n}\r\n\r\n.bar2 {\r\n    right: 0;\r\n    width: 25px;\r\n    height: 5px;\r\n    background-color: #00a19b;\r\n    margin: 6px 0;\r\n    transition: 0.4s;\r\n    border-radius: 4px;\r\n}\r\n\r\n.change .bar1 {\r\n    transform: translate(0, 11px) rotate(-45deg);\r\n}\r\n\r\n.change .bar2 {\r\n    opacity: 0;\r\n}\r\n\r\n.change .bar3 {\r\n    transform: translate(0, -11px) rotate(45deg);\r\n}\r\n\r\n\r\n\r\n@media only screen and (max-width: 993px) {\r\n    .noRespon {\r\n        ul {\r\n            display: none;\r\n            animation-name: menuTran;\r\n            animation-duration: 0.8s;\r\n            animation-timing-function: ease;\r\n            animation-direction: reverse;\r\n        }\r\n    }\r\n}\r\n\r\n@media only screen and (max-width: 993px) {\r\n\r\n    .navigation .logo img {\r\n        padding-top: 0px;\r\n        height: 85px;\r\n    }\r\n\r\n@keyframes menuTran {\r\n    0%{\r\n        transform: translateY(50px);\r\n    }\