{"ast":null,"code":"import Vue from 'vue';\nimport axios from 'axios';\nimport VueAxios from 'vue-axios';\nVue.use(VueAxios, axios);\nimport theBooks from '../../';\nimport { Hooper, Slide, Progress as HooperProgress, Pagination as HooperPagination, Navigation as HooperNavigation } from 'hooper';\nimport 'hooper/dist/hooper.css';\nexport default {\n  components: {\n    Hooper,\n    Slide,\n    HooperProgress,\n    HooperPagination,\n    HooperNavigation\n  },\n  name: 'readBook',\n  data() {\n    return {\n      infos: null,\n      books: theBooks,\n      hooperSettings: {\n        playSpeed: 3000,\n        progress: true,\n        itemsToShow: '3',\n        centerMode: true,\n        infiniteScroll: true\n      }\n    };\n  },\n  computed: {\n    coputedHei() {\n      return this.height;\n    }\n  },\n  filters: {\n    shorten: function (value, textLength, suffix) {\n      return value.substring(0, textLength) + suffix;\n    }\n  }\n};","map":{"version":3,"names":["Vue","axios","VueAxios","use","theBooks","Hooper","Slide","Progress","HooperProgress","Pagination","HooperPagination","Navigation","HooperNavigation","components","name","data","infos","books","hooperSettings","playSpeed","progress","itemsToShow","centerMode","infiniteScroll","computed","coputedHei","height","filters","shorten","value","textLength","suffix","substring"],"sources":["src/components/global/readBook.vue"],"sourcesContent":["<template>\r\n    <div class=\"container readBook bg-white\">\r\n        <div class=\"row\">\r\n            <h3 class=\"pt-2\">كتبنا</h3>\r\n            <p class=\"fw-light pb-4\">هذه الكتب التي قرأناها في المسار الفكري في نادينا الشبابي</p>\r\n            <hooper class=\"hooper3\" :infiniteScroll=\"true\" :settings=\"hooperSettings\">\r\n                <slide class=\"col-md-4\" v-for=\"book in books\" :key=\"book.id\">\r\n                    <div class=\"pageBook\">\r\n                        <span dir=\"rtl\"> عدد الصفحات: {{book.pageNumber}}</span>\r\n                