n    margin-right: 10px;\r\n    position: relative;\r\n    padding: 20px;\r\n    box-shadow: 0px 1px 1px 1px #009688;\r\n    margin-left: 10px;\r\n}\r\n\r\n\r\n    span {\r\n        font-size: 16px;\r\n        position: absolute;\r\n        top: -4px;\r\n        right: 85px;\r\n        font-weight: 300;\r\n        font-family: system-ui;\r\n    }\r\n    h5{\r\n        font-size:18px ;\r\n        padding-bottom: 10px;\r\n        padding-top: 10px;\r\n        border-bottom: solid #00a19b 2px;\r\n        color:black ;\r\n    }\r\n    p{\r\n        font-size: 16px;\r\n        font-family: system-ui;\r\n        padding: 50px;\r\n        padding-top: 20px;\r\n        margin-right:15px ;\r\n        color: #777777;\r\n        \r\n    }\r\n}\r\n\r\n\r\n\r\n\r\n.hooper3 .hooper-list{\r\n\r\nheight: 236.5%;\r\n}\r\n\r\n    .hooper3:last-child img {\r\n        margin-top: 0 !important;\r\n    }\r\n    \r\n    \r\n    .hooper3:last-child img {\r\n        width: 100% !important; \r\n        height: 100% !important;\r\n        opacity: 1 !important;\r\n    }\r\n    \r\n    .hooper3 .hooper-next, .hooper-prev {\r\n        top: 65% !important;\r\n        \r\n        font-size: 20px;\r\n    }\r\n\r\n    .hooper3 .icon{\r\n        background-color: #00a19b  !important;\r\n        color: white !important;\r\n    }\r\n    \r\n    .hooper3 .icon:hover{\r\n        background-color: white  !important;\r\n        border: solid 3px #00a19b;\r\n    }\r\n    \r\n    \r\n    .is-current img {\r\n        opacity: 1 !important;\r\n    }\r\n\r\n\r\n    .hooper3 .btn-success {\r\n        --bs-btn-color: #fff;\r\n        --bs-btn-bg: #00a19b;\r\n        --bs-btn-border-color: #ffffff;\r\n        --bs-btn-hover-color: #00a19b;\r\n        --bs-btn-hover-bg: #ffffff;\r\n        --bs-btn-hover-border-color: #00a19b;\r\n        --bs-btn-focus-shadow-rgb: 60, 153, 110;\r\n    }\r\n    \r\n    \r\n    @media (max-width: 1399.98px) {\r\n        .hooper:last-child img {\r\n            margin-top: 24%;\r\n   