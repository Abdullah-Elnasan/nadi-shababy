hooper-pagination>\r\n            </hooper>\r\n            <!-- 0px 3px 18px 1px rgba(0, 0, 0, 0.1) -->\r\n        </div>\r\n    </div>\r\n</template>\r\n<script>\r\nimport Vue from 'vue'\r\nimport axios from 'axios'\r\nimport VueAxios from 'vue-axios'\r\nVue.use(VueAxios, axios)\r\n\r\nimport {\r\n    Hooper,\r\n    Slide,\r\n    Progress as HooperProgress,\r\n    Pagination as HooperPagination,\r\n    Navigation as HooperNavigation\r\n} from 'hooper';\r\n\r\nimport 'hooper/dist/hooper.css';\r\n\r\nexport default {\r\n\r\n    components: {\r\n        Hooper,\r\n        Slide,\r\n        HooperProgress,\r\n        HooperPagination,\r\n        HooperNavigation\r\n    },\r\n\r\n    name: 'readBook',\r\n    data() {\r\n        return {\r\n            infos: null,\r\n            mesaage: 'fsd',\r\n            hooperSettings: {\r\n                playSpeed: 3000,\r\n                progress: true,\r\n                itemsToShow: '3',\r\n                centerMode: true,\r\n                infiniteScroll: true,\r\n            }\r\n        }\r\n    },\r\n    mounted() {\r\n        axios.get('http://localhost:3000/books').then(response => (this.infos = response.data)).catch(error => { console.log('ed5dd') }).finally(console.log('succes'))\r\n    }\r\n}\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n.readBook {\r\n    position: relative;\r\n\r\n    p {\r\n        font-size: 20px;\r\n    }\r\n}\r\n</style>"],"mappings":"AAmBA,OAAAA,GAAA;AACA,OAAAC,KAAA;AACA,OAAAC,QAAA;AACAF,GAAA,CAAAG,GAAA,CAAAD,QAAA,EAAAD,KAAA;AAEA,SACAG,MAAA,EACAC,KAAA,EACAC,QAAA,IAAAC,cAAA,EACAC,UAAA,IAAAC,gBAAA,EACAC,UAAA,IAAAC,gBAAA,QACA;AAEA;AAEA;EAEAC,UAAA;IACAR,MAAA;IACAC,KAAA;IACAE,cAAA;IACAE,gBAAA;IACAE;EACA;EAEAE,IAAA;EACAC,KAAA;IACA;MACAC,KAAA;MACAC,OAAA;MACAC,cAAA;QACAC,SAAA;QACAC,QAAA;QACAC,WAAA;QACAC,UAAA;QACAC,cAAA;MACA;IACA;EACA;EACAC,QAAA;IACAtB,KAAA,CAAAuB,GAAA,gCAAAC,IAAA,CAAAC,QAAA,SAAAX,KAAA,GAAAW,QAAA,CAAAZ,IAAA,EAAAa,KAAA,CAAAC,KAAA;MAAAC,OAAA,CAAAC,GAAA;IAAA,GAAAC,OAAA,CAAAF,OAAA,CAA